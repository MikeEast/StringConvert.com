<!DOCTYPE html>
<html>
<head>
    <title>StringConvert</title>
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <link rel="stylesheet/less" href="@Url.Content("~/Content/less/bootstrap.less")" media="all" />
    <script src="@Url.Content("~/Scripts/less-1.1.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/knockout-2.0.0.debug.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/knockout.mapping-latest.debug.js")" type="text/javascript"></script>
